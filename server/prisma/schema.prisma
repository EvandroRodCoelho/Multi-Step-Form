// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id          String @id @default(cuid())
  fullName    String
  email       String @unique
  password    String
  gender      String
  zipCode     String
  city        String
  country     String
  state       String
  urlLinkedin String
  urlGitHub   String
}
