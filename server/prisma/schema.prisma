// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL") // Configure a conex√£o do banco de dados aqui
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id          String @id @default(cuid())
  fullName    String
  email       String @unique
  password    String
  gender      String
  zipCode     String
  city        String
  country     String
  state       String
  urlLinkedin String
  urlGitHub   String
}
